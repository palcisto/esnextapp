{
  "name": "esnextapp",
  "version": "1.0.0",
  "description": "Boilerplate Javascript app with ES Next support",
  "main": "index.js",
  "repository": "git@github.com:palcisto/esnextapp.git",
  "author": "palcisto <patrickalcisto@gmail.com>",
  "license": "MIT",
  "scripts": {
    "build": "node .node-version.js gulp build",
    "preinstall": "node .node-version.js",
    "prestart": "node .node-version.js",
    "start": "gulp watch",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "devDependencies": {
    "babel-jest": "^20.0.3",
    "babel-loader": "^7.0.0",
    "babel-preset-env": "^1.5.1",
    "babel-preset-stage-2": "^6.24.1",
    "del": "^2.2.2",
    "gulp": "^3.9.1",
    "gulp-util": "^3.0.8",
    "jest": "^20.0.4",
    "jest-cli": "^20.0.4",
    "run-sequence": "^1.2.2",
    "webpack": "^2.6.1"
  },
  "jest": {
    "moduleFileExtensions": [
      "js"
    ],
    "moduleDirectories": [
      "node_modules"
    ],
    "modulePathIgnorePatterns": [
      "<rootDir>/build/"
    ],
    "modulePaths": [
      "<rootDir>/src/"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "identity-obj-proxy"
    }
  }
}
